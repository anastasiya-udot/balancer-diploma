	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: start agent",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/tools/agent/agent.py",
			"args": [
				"--config", "${workspaceFolder}/tools/agent/agent.json",
				"--test", "local"
			],
			"console": "integratedTerminal",
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"program": "${workspaceFolder}/bin/www",
			"console": "integratedTerminal",
			"stopOnEntry": false
		}
	]
}